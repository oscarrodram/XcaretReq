<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="V1"
		title="{i18n>PSPNR}"
		items="{serviceModel>/fPro}"
		search="_handleProyecto"
		confirm="_handleDialogClose"
		cancel="onCancel"
		multiSelect="true">
        <customData>
			<core:CustomData
				key="fragmentPath"
				value="pspnr" />	<!--Need send the Fragment ID items="{serviceModelPsr>/Psnr}"-->
		</customData>
		<StandardListItem
			title="{serviceModel>PSPNR}"
			description="{serviceModel>PROJECT_NAME}"
            class="hideTitle"/>
	</SelectDialog>
</core:FragmentDefinition>