<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="V7"
		title="{i18n>DIVISION_DESC}"
		items="{serviceModel>/fDiv}"
		search="_handleDiv"
		confirm="_handleDialogClose"
		cancel="onCancel"
		multiSelect="true">
        <customData>
			<core:CustomData
				key="fragmentPath"
				value="DIVSN" />	<!--Need send the Fragment ID items="{serviceModelPsr>/Psnr}"-->
		</customData>
		<StandardListItem
			title="{serviceModel>DIVSN}"
			description="{serviceModel>DIVISION_DESC}"
            class="hideTitle"/>
	</SelectDialog>
</core:FragmentDefinition>