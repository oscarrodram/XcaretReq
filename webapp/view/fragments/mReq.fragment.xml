<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="V2"
		title="{i18n>BANFN}"
		items="{serviceModel>/fReq}"
		search="_handleReq"
		confirm="_handleDialogClose"
		cancel="onCancel"
		multiSelect="true">
        <customData>
			<core:CustomData
				key="fragmentPath"
				value="Req" />	<!--Need send the Fragment ID items="{serviceModelPsr>/Psnr}"-->
		</customData>
		<StandardListItem
			title="{serviceModel>BANFN}"
			description="{serviceModel>BANFN}"
            class="hideTitle"/>
	</SelectDialog>
</core:FragmentDefinition>