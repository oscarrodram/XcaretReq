<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="V5"
		title="{i18n>FAMILY_DESC}"
		items="{serviceModel>/fFam}"
		search="_handleFam"
		confirm="_handleDialogClose"
		cancel="onCancel"
		multiSelect="true">
        <customData>
			<core:CustomData
				key="fragmentPath"
				value="Family" />	<!--Need send the Fragment ID items="{serviceModelPsr>/Psnr}"-->
		</customData>
		<StandardListItem
			title="{serviceModel>FAMILY_ID}"
			description="{serviceModel>FAMILY_DESC}"
            class="hideTitle"/>
	</SelectDialog>
</core:FragmentDefinition>