<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="V4"
		title="{i18n>CATEGORY_DESC}"
		items="{serviceModel>/fCat}"
		search="_handleCat"
		confirm="_handleDialogClose"
		cancel="onCancel"
		multiSelect="true">
        <customData>
			<core:CustomData
				key="fragmentPath"
				value="CATEGORY_ID" />	<!--Need send the Fragment ID items="{serviceModelPsr>/Psnr}"-->
		</customData>
		<StandardListItem
			title="{serviceModel>CATEGORY_ID}"
			description="{serviceModel>CATEGORY_DESC}"
            class="hideTitle"/>
	</SelectDialog>
</core:FragmentDefinition>