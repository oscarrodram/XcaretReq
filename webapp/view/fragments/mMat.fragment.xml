<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="V3"
		title="{i18n>MATNR}"
		items="{serviceModel>/fMat}"
		search="_handleMat"
		confirm="_handleDialogClose"
		cancel="onCancel"
		multiSelect="true">
        <customData>
			<core:CustomData
				key="fragmentPath"
				value="Matnr" />	<!--Need send the Fragment ID items="{serviceModelPsr>/Psnr}"-->
		</customData>
		<StandardListItem
			title="{serviceModel>MATNR}"
			description="{serviceModel>MATERIAL_NAME}"
            class="hideTitle"/>
	</SelectDialog>
</core:FragmentDefinition>