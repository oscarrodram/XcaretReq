<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="V10"
		title="{i18n>SUBIC}"
		items="{serviceModel>/fSub}"
		search="_handleSubi"
		confirm="_handleDialogClose"
		cancel="onCancel"
		multiSelect="true">
        <customData>
			<core:CustomData
				key="fragmentPath"
				value="SUBI" />	<!--Need send the Fragment ID items="{serviceModelPsr>/Psnr}"-->
		</customData>
		<StandardListItem
			title="{serviceModel>SUBIC}"
			description="{serviceModel>SUBIC}"
            class="hideTitle"/>
	</SelectDialog>
</core:FragmentDefinition>